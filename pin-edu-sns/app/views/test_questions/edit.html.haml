- htitle '题目录入'

- hgoback admin_test_questions_path
- hbreadcrumb '首页', '/admin'
- hbreadcrumb '题目录入', "/admin/test_questions/#{@test_question.id}/edit"
- hbreadcrumb '编辑题目'
  
.page-model-form
  = flash_info
  = form_for @test_question do |t|
    .field
      %label 题目标题
      = t.text_field :title


    .field.submit
      = jfsubmit '确定'
      = jfcancel '取消'
